<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xsi:schemaLocation="
        http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">

    <!-- bean definitions here -->

    <bean id="specification" class="car.example.autowire.byName.Specification">
       <property name="make" value="Lamborghini"></property>
        <property name="model" value="Urus"></property>

    </bean>

    <bean id="specification1" class="car.example.autowire.byName.Specification">
        <property name="make" value="Ferrari"></property>
        <property name="model" value="Red"></property>

    </bean>

    <bean id="MyCar" class="car.example.autowire.byName.Car" autowire="byName">

    </bean>

</beans>